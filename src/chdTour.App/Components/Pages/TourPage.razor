@page "/tour"
@using chdTour.App.Components.Pages.Base
@using chdTour.App.Components.Pages.Scala
@using chdTour.App.Components.Pages.Tours
@using chdTour.DataAccess.Contracts.Domain
@inherits BaseTourSerachForm<ITourRepository, Tour>

<EntitySearch  T="Tour" PK="Guid" OnReload="this.Reload" Columns="this.Columns" IsGroupable="true"
             TRepo="ITourRepository" Item="this._item" Items="this._items" OnEdit="async x => await this.SetEntity(x)">
    <ChildContent>
        <TourEdit Token="this._cts.Token" OnBack="async ()=>await this.OnBack()" Value="this._item" />
    </ChildContent>
</EntitySearch>