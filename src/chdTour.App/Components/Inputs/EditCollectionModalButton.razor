@using chd.UI.Base.Components.General
@using chdTour.App.Components.Base
@using chdTour.DataAccess.Contracts.Interfaces.Repositories.Base

@typeparam TRepo where TRepo : IBaseRepository<T>
@typeparam TParentRepo where TParentRepo : IBaseRepository<TParent>
@typeparam T where T : class
@typeparam TParent where TParent : class
@typeparam TModalForm where TModalForm : BaseEditCollectionForm<TRepo, TParentRepo, T, TParent>
@inherits BaseEditModalButton<TParentRepo, TParent>

<EditFormInput Title="@this.Title">
    <IconButton OnClick="this.Open" FAClass="@(string.IsNullOrEmpty(this.FAIcon) ? "up-right-from-square" : this.FAIcon)"></IconButton>
</EditFormInput>
